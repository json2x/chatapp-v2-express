{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "https://mrroboto.vercel.app" },
                { "key": "Access-Control-Allow-Methods", "value": "*" },
                { "key": "Access-Control-Allow-Headers", "value": "*" },
                { "key": "Access-Control-Allow-Credentials", "value": "true" }
            ]
        }
    ]
}